{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es2021",
    "lib": [
      "es2021",
      "dom"
    ],
    "module": "commonjs",
    "sourceMap": true,
    "noImplicitAny": true,
    "strict": true,
    "esModuleInterop": true,
    "preserveConstEnums": true,
    "strictNullChecks": false,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": [
      "./node_modules/@types",
      "./src/types"
    ],
  },
  "display": "Node 16",
  "$schema": "https://json.schemastore.org/tsconfig"
}
